{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/account/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/account/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/account",
  "projects": {
    "account": {
      "type": "application",
      "root": "apps/account",
      "entryFile": "main",
      "sourceRoot": "apps/account/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account/tsconfig.app.json"
      }
    },
    "drawing": {
      "type": "application",
      "root": "apps/drawing",
      "entryFile": "main",
      "sourceRoot": "apps/drawing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/drawing/tsconfig.app.json"
      }
    },
    "game": {
      "type": "application",
      "root": "apps/game",
      "entryFile": "main",
      "sourceRoot": "apps/game/src",
      "compilerOptions": {
        "tsConfigPath": "apps/game/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    }
  }
}